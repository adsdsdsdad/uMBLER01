{
  "name": "sistema-atendimento-tempo-real",
  "version": "1.0.0",
  "description": "Sistema completo de atendimento ao cliente com integração em tempo real, métricas de performance e dashboard de monitoramento",
  "keywords": [
    "atendimento",
    "chat",
    "tempo-real",
    "umbler",
    "dashboard",
    "metricas",
    "nextjs",
    "typescript"
  ],
  "author": "Equipe de Desenvolvimento",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:setup": "psql $DATABASE_URL -f scripts/setup-database.sql",
    "db:backup": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d).sql",
    "db:restore": "psql $DATABASE_URL -f backup_*.sql",
    "clean": "rm -rf .next out",
    "test": "echo \"Testes não configurados ainda\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
